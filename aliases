# Unix
alias ll="ls -al"
alias ln="ln -v"
alias mkdir="mkdir -p"
alias e="$EDITOR"

# Ritchie's stuff :)

# Change wallpaper
alias w="$HOME/.bin/change_wallpaper.sh"

# Habit change
alias v="echo \"Use 'e'\"! Gawd!"

# Pretty print the path
alias path='echo $PATH | tr -s ":" "\n"'

# Mail!
alias m='neomutt'

