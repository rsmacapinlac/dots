# Unix
alias ll="ls -al"
alias ln="ln -v"
alias mkdir="mkdir -p"
alias e="$EDITOR"

# Ritchie's stuff :)
# alias w="feh --bg-scale --bg-fill --randomize ~/Nextcloud/Wallpapers"
alias w="$HOME/.bin/change_wallpaper.sh"

# Habit change
alias v="echo \"Use 'e'\"! Gawd!"

# Pretty print the path
alias path='echo $PATH | tr -s ":" "\n"'
