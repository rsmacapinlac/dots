# Unix
alias ll="ls -al"
alias ln="ln -v"
alias mkdir="mkdir -p"
alias e="$EDITOR"
alias r="ranger"
alias obsidian="cd '$HOME/Nextcloud/Apps/obsidian/Second Brain/' && nvim"

# Ritchie's stuff :)

